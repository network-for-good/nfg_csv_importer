<div class="col-importer" data-column-name="<%= field_name %>">
  <div class="card card-unmapped <%= highlight_class %>" data-mapped="false">
    <div class="card-header" id="card_header_<%= field_id %>">
      <div class="card-header-interactions">
        <label class="c-input c-checkbox m-b-quarter">
          <input name="import[fields_mapping[<%= field_name %>]]" type="checkbox" value="ignore_column">
          <span class="c-indicator"></span>
          <span class="text-muted">Ignore Column</span>
        </label>
      </div>
      <div class="form-group">
        <select name="import[fields_mapping[<%= field_name %>]]" id="import_fields_mapping_<%= field_id %>" class="c-select" data-describe="select-column-header"><option value="">Select Header</option><option value=""></option>
        <option value="amount">Amount</option>
        <option value="donated_at">Donated At</option>
        <option value="ignore_column">Ignore Column</option></select>
      </div>
      <h4 class="h5 text-xs-center">
        <%= field_name %>
        <sup>
          <i class="fa fa-info-circle" data-placement="top" data-toggle="tooltip" title="" data-original-title="This column hasn't been mapped yet"></i>
        </sup>
      </h4>
    </div>
    <div class="card-block card-block-importer-index">
      C
    </div>
    <div class="list-group list-group-flush">
      <div class="list-group-item">
        5307 Falls Road Terrace
      </div>
      <div class="list-group-item">
        5307 Falls Road Terrace
      </div>
      <div class="list-group-item">

      </div>
      <div class="list-group-item">
        123 This Street
      </div>
      <div class="list-group-item">
        545 At That Corner
      </div>
    </div>
  </div>
</div>